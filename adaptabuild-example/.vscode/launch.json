  "version": "0.2.0",

  "configurations": [
    {
      "name": "Debug foo on STM32F0",
      "cwd": "${workspaceFolder}/projects/adaptabuild-example",
      "executable": "build/foo/STM32F051R8/product/foo/foo",
      "request": "launch",
      "type": "cortex-debug",
      "runToEntryPoint": "main",
      "servertype": "external",
      "gdbTarget": "host.gdb.gateway:4242",
      "gdbPath": "/usr/bin/gdb-multiarch",
//    "showDevDebugOutput": "raw",
    },
  ],
}

